- model_class = Cpu
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize
%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:datetime)
      %th= model_class.human_attribute_name(:smfid)
      %th= model_class.human_attribute_name(:mips)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - @cpus.each do |cpu|
      %tr
        %td= link_to cpu.id, cpu_path(cpu)
        %td= cpu.datetime
        %td= cpu.smfid
        %td= cpu.mips
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_cpu_path(cpu), :class => 'btn btn-mini'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), cpu_path(cpu), :method => :delete, :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')), :class => 'btn btn-mini btn-danger'
= will_paginate @cpus, :renderer => BootstrapPagination::Rails
= link_to t('.new', :default => t("helpers.links.new")), new_cpu_path, :class => 'btn btn-primary'
