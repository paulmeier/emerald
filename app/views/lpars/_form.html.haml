= simple_form_for @lpar, html: { class: 'form-horizontal' } do |f|
  = f.input :mid, label: "MID"
  = f.input :smfid, label: "SMFID"
  = f.input :name, label: "Name"
  .form-actions
    = f.button :submit, class: 'btn-primary'
    = link_to t('.cancel', default: t("helpers.links.cancel")), lpars_path, class: 'btn'
